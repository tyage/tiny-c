int test_calc(int x, int y) {
  int z;
  z = y + 3;
  return z * x + (y * 7 / 4);
}
int test_if(int x) {
  if (x == 1) {
    return 1;
  } else {
    return 0;
  }
}
int test_while(int x) {
  int y;
  y = 0;
  while (y < x * 5) {
    y = y + 1;
  }
  return y + 2;
}
int test_call(int x) {
  if (test_while(x) > 7) {
    return 10;
  } else {
    return test_calc(x, x);
  }
}
int global;
int test_global() {
  global = global + 1;
  return global;
}
